{
  "name": "beautify-api",
  "public": true,
  "builds": [
    {
      "src": "src/infra/http/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "src/infra/http/server.ts"
    }
  ]
}